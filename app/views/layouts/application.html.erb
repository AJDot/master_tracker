<!DOCTYPE html>
<html>
  <head>
    <title>MasteryTracker</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag "https://use.fontawesome.com/fa01417f2f.js"
 %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="navbar">
      <div class="navbar-inner">
        <%= render 'layouts/navigation' %>
      </div>
    </div>
    <main>
      <%= render 'layouts/flash_messages' %>
      <ul>
        <li>Edit/Update Categories/Skills/Descriptions</li>
        <li>Edit/Update Spreadsheet Name</li>
      </ul>
      <%= yield %>
    </main>
    <footer>
      <%= render 'layouts/footer' %>
    </footer>
  </body>
</html>
